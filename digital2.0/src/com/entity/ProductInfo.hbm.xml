<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.entity">
	<class	name="ProductInfo" table="product_info" catalog="digital">
		<id name="id" type="java.lang.Integer">
			<column name="id"/>
			<generator class="native"/>
		</id>
		<property name="code" type="java.lang.String">
			<column name="code" length="16" not-null="true"/>
		</property>
		<property name="name" type="java.lang.String">
			<column name="name" length="255"/>
		</property>
		<property name="brand" type="java.lang.String">
			<column name="brand" length="20"/>
		</property>
		<property name="pic" type="java.lang.String">
			<column name="pic" length="255"/>
		</property>
		<property name="num" type="java.lang.Integer">
			<column name="num"/>
		</property>
		<property name="price" type="java.lang.Double">
			<column name="price" length="50"/>
		</property>
		<property name="intro" type="java.lang.String">
			<column name="intro"/>
		</property>
		<property name="status" type="java.lang.Integer">
			<column name="status"/>
		</property>
		<property name="bigpic" type="java.lang.String">
			<column name="bigpic" length="255"/>
		</property>
		
		<many-to-one name="type" column="tid" class="ProductType" lazy="false"></many-to-one>
		
	</class>
</hibernate-mapping>